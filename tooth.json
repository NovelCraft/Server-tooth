{
    "format_version": 1,
    "tooth": "github.com/novelcraft/server-tooth",
    "version": "0.6.0",
    "information": {
        "name": "NovelCraft Server",
        "description": "NovelCraft Servers allow players to play online or via a local area network with other people.",
        "author": "NovelCraft",
        "license": "LGPL-3.0",
        "homepage": "https://novelcraft.games"
    },
    "placement": [
        {
            "source": "common/*",
            "destination": "*"
        },
        {
            "source": "linux-arm/*",
            "destination": "*",
            "GOOS": "linux",
            "GOARCH": "arm"
        },
        {
            "source": "linux-arm64/*",
            "destination": "*",
            "GOOS": "linux",
            "GOARCH": "arm64"
        },
        {
            "source": "linux-x64/*",
            "destination": "*",
            "GOOS": "linux",
            "GOARCH": "amd64"
        },
        {
            "source": "osx-arm64/*",
            "destination": "*",
            "GOOS": "darwin",
            "GOARCH": "arm64"
        },
        {
            "source": "osx-x64/*",
            "destination": "*",
            "GOOS": "darwin",
            "GOARCH": "amd64"
        },
        {
            "source": "win-arm/*",
            "destination": "*",
            "GOOS": "windows",
            "GOARCH": "arm"
        },
        {
            "source": "win-arm64/*",
            "destination": "*",
            "GOOS": "windows",
            "GOARCH": "arm64"
        },
        {
            "source": "win-x64/*",
            "destination": "*",
            "GOOS": "windows",
            "GOARCH": "amd64"
        },
        {
            "source": "win-x86/*",
            "destination": "*",
            "GOOS": "windows",
            "GOARCH": "386"
        }
    ]
}
